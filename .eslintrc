{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 5,
    "ecmaFeatures": {
      "jsx": false
    },
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": false
  },
  "globals": {
    "jQuery": false,
    "$": false
  },
  "plugins": [],
  "rules": {
    // Possible errors
    "valid-jsdoc": 1,
    // Standard
    "accessor-pairs": 0,
    "array-callback-return": 1,
    "block-scoped-var": 1,
    "complexity": 1,
    "consistent-return": 1,
    "curly": 1,
    "default-case": 1,
    "dot-location": 0,
    "dot-notation": 1,
    "eqeqeq": 1,
    "guard-for-in": 1,
    "no-alert": 1,
    "no-caller": 1,
    "no-div-regex": 1,
    "no-empty-pattern": 1,
    "no-eq-null": 1,
    "no-eval": 1,
    "no-extend-native": 1,
    "no-extra-bind": 1,
    "no-extra-label": 1,
    "no-floating-decimal": 1,
    //"no-implicit-coercion:": 1,
    "no-implicit-globals": 1,
    "no-implied-eval": 1,
    "no-invalid-this": 1,
    "no-iterator": 1,
    "no-labels": 1,
    "no-lone-blocks": 1,
    "no-loop-func": 1,
    "no-magic-numbers": 0,
    "no-multi-spaces": 1,
    "no-multi-str": 1,
    "no-native-reassign": 1,
    "no-new": 1,
    "no-new-func": 1,
    "no-new-wrappers": 1,
    "no-octal-escape": 1,
    "no-param-reassign": 1,
    "no-proto": 1,
    "no-return-assign": 1,
    "no-script-url": 1,
    "no-sequences": 1,
    "no-throw-literal": 1,
    "no-unmodified-loop-condition": 1,
    "no-unused-expressions": 1,
    "no-useless-call": 1,
    "no-useless-concat": 1,
    "no-useless-escape": 1,
    "no-void": 1,
    "no-warning-comments": 0,
    "no-with": 1,
    "radix": 1,
    //    "vars-on-top": 1,
    "wrap-iife": 1,
    "yoda": 1,
    // Strict mode
    "strict": 0,
    // Variables
    "init-declarations": 0,
    "no-catch-shadow": 1,
    "no-label-var": 1,
    "no-restricted-globals": 1,
    "no-shadow": 1,
    "no-shadow-restricted-names": 1,
    "no-undef": 1,
    "no-undef-init": 1,
    "no-undefined": 1,
    "no-use-before-define": 1,
    // Stylistic Issues
    "array-bracket-spacing": 1,
    "block-spacing": 1,
    "brace-style": 1,
    "camelcase": 1,
    "comma-spacing": 1,
    "comma-style": 1,
    "computed-property-spacing": 1,
    "consistent-this": 1,
    "eol-last": 1,
    //"func-names": 1,
    //    "func-style": 1,
    "id-blacklist": 1,
    "indent": 1,
    "jsx-quotes": 1,
    "key-spacing": 1,
    "keyword-spacing": 1,
    "linebreak-style": 1,
    "lines-around-comment": 1,
    "max-depth": 0,
    "max-len": 0,
    "max-nested-callbacks": 1,
    "max-params": 0,
    "max-statements": 0,
    "max-statements-per-line": 1,
    "new-cap": 0,
    "new-parens": 0,
    //    "newline-after-var": 1,
    "newline-before-return": 0,
    "newline-per-chained-call": 0,
    "no-array-constructor": 0,
    "no-bitwise": 1,
    "no-continue": 1,
    "no-inline-comments": 0,
    "no-lonely-if": 1,
    "no-multiple-empty-lines": 1,
    "no-negated-condition": 1,
    "no-nested-ternary": 1,
    "no-new-object": 1,
    "no-plusplus": 0,
    "no-restricted-syntax": 0,
    "no-spaced-func": 1,
    "no-ternary": 0,
    "no-trailing-spaces": 1,
    //    "no-underscore-dangle": 1,
    "no-unneeded-ternary": 1,
    "no-whitespace-before-property": 1,
    "object-curly-spacing": 0,
    //    "one-var": 1,
    //    "one-var-declaration-per-line": 1,
    "operator-assignment": 0,
    "operator-linebreak": 1,
    "padded-blocks": 1,
    "quote-props": 0,
    "quotes": [1, "single"],
    "require-jsdoc": 1,
    "semi": 1,
    "semi-spacing": 1,
    "no-extra-semi": 0,
    "sort-vars": 0,
    "space-before-blocks": 1,
    "space-before-function-paren": 1,
    "space-in-parens": 1,
    "space-infix-ops": 1,
    "space-unary-ops": 1,
    "spaced-comment": 1,
    "wrap-regex": 1,
    "no-console": 0
  }
}
